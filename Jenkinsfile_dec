pipeline {
    agent any
    environment {
      registry = "los911/laravel-jenkins"
      registryCredential = 'dockerhub'
      dockerImage = ''
    }
    stages {
        stage('Checkout Source') {
            steps {
                git credentialsId: 'github', url:'https://github.com/los911evgen/laravel-jenkins.git', branch:'main'
             }
        }
        stage('Развертывание') {
            steps {
                sh 'ls'            
                sh './deploy.sh'
            }
        }
    }
}
